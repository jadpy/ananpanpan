})

        local MainTab = Window:MakeTab({
Name = "掴むモット",
Icon = "rbxassetid://4483345998",
PremiumOnly = false
})

local enabled = false

-- MainTabにトグルスイッチを追加
MainTab:AddToggle({
    Name = "キル掴む",
    Default = false,
    Callback = function(Value)
        enabled = Value
        print(Value)
    end
})

-- WorkspaceのChildAddedイベントを監視
game:GetService("Workspace").ChildAdded:Connect(function(model)
    if enabled and model.Name == "GrabParts" then
        local part_to_impulse = model["GrabPart"]["WeldConstraint"].Part1
        if part_to_impulse then
            part_to_impulse.Parent:BreakJoints()    
        end    
    end    
end)

local enabled = false
local connection

-- MainTabにトグルスイッチを追加
MainTab:AddToggle({
    Name = "上に上がる掴む",
    Default = false,
    Callback = function(Value)
        enabled = Value
        if enabled then
            if not connection then
                connection = game:GetService("Workspace").ChildAdded:Connect(function(model)
                    if model.Name == "GrabParts" then
                        local part_to_impulse = model:FindFirstChild("GrabPart")
                        if part_to_impulse and part_to_impulse:FindFirstChild("WeldConstraint") then
                            local bodyVelocity = Instance.new("BodyVelocity")
                            bodyVelocity.Parent = part_to_impulse
                            bodyVelocity.Velocity = Vector3.new(0, 200, 0) -- 吹っ飛ばす方向や速度を設定できます
                            bodyVelocity.MaxForce = Vector3.new(0, math.huge, 0) -- 必要に応じて調整してください
                        end
                    end
                end)
            end
        else
            if connection then
                connection:Disconnect()
                connection = nil
            end
        end
        print("Sky Grab! enabled:", enabled)
    end
})

MainTab:AddToggle({
    Name = "下に下がる掴む",
    Default = false,
    Callback = function(Value)
        enabled = Value
        if enabled then
            if not connection then
                connection = game:GetService("Workspace").ChildAdded:Connect(function(model)
                    if model.Name == "GrabParts" then
                        local part_to_impulse = model:FindFirstChild("GrabPart")
                        if part_to_impulse and part_to_impulse:FindFirstChild("WeldConstraint") then
                            local bodyVelocity = Instance.new("BodyVelocity")
                            bodyVelocity.Parent = part_to_impulse
                            bodyVelocity.Velocity = Vector3.new(0, -200, 0) -- 吹っ飛ばす方向や速度を設定できます
                            bodyVelocity.MaxForce = Vector3.new(0, math.huge, 0) -- 必要に応じて調整してください
                        end
                    end
                end)
            end
        else
            if connection then
                connection:Disconnect()
                connection = nil
            end
        end
        print("Sky Grab! enabled:", enabled)
    end
})

local enabled = false

-- MainTabにトグルスイッチを追加
MainTab:AddToggle({
    Name = "回転掴む",
    Default = false,
    Callback = function(Value)
        enabled = Value
        print(Value)
    end
})

-- WorkspaceのChildAddedイベントを監視
game:GetService("Workspace").ChildAdded:Connect(function(model)
    if enabled and model.Name == "GrabParts" then
        local part_to_impulse = model["GrabPart"]["WeldConstraint"].Part1
        if part_to_impulse then
            -- 回転させるための力を加える
            local bodyAngularVelocity = Instance.new("BodyAngularVelocity")
            bodyAngularVelocity.Parent = part_to_impulse
            bodyAngularVelocity.AngularVelocity = Vector3.new(0, 10000, 0) -- 回転の速度を設定（Y軸周りに1000度/秒）
            bodyAngularVelocity.MaxTorque = Vector3.new(0, math.huge, 0) -- 必要に応じてトルクを調整

            -- モデルの全ての子要素を削除する
            for _, child in ipairs(model:GetChildren()) do
                child:Destroy()
            end
        end
    end
end)

local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")

local function HandleGrabParts(model)
    local part_to_impulse = model.GrabPart.WeldConstraint.Part1
    if part_to_impulse then
        local humanoid = part_to_impulse.Parent:FindFirstChildOfClass("Humanoid")
        if humanoid then
            -- プレイヤーを上にテレポート
            local character = part_to_impulse.Parent
            local rootPart = character:FindFirstChild("HumanoidRootPart")
            if rootPart then
                -- テレポート先の上の位置を計算
                local upVector = Vector3.new(0, 60, 0) -- 上に20単位移動するベクトル
                local newPosition = rootPart.Position + upVector

                -- プレイヤーをテレポート
                rootPart.CFrame = CFrame.new(newPosition)

                -- プレイヤーを浮遊状態に設定する
                humanoid.PlatformStand = true
                humanoid.AutoRotate = false

                -- しゃがまないようにする
                humanoid.AutoCrouch = false
                humanoid.Sit = false

                -- 触れた人がragdoll化しないようにする
                humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
            end
        end
    end
end

-- 新しい子が追加されたときのイベント
Workspace.ChildAdded:Connect(function(model)
    if model.Name == "GrabParts" then
        -- Toggleの状態に応じて動作を切り替える
        local toggleState = MainTab.ToggleState or false  -- デフォルトはfalse

        if toggleState then
            HandleGrabParts(model)
        end
    end
end)

-- 初期のマップ探索
for _, model in pairs(Workspace:GetChildren()) do
    if model.Name == "GrabParts" then
        -- Toggleの状態に応じて動作を切り替える
        local toggleState = MainTab.ToggleState or false  -- デフォルトはfalse

        if toggleState then
            HandleGrabParts(model)
        end
    end
end

-- Toggleの状態が変わったときに呼ばれるコールバック関数
MainTab:AddToggle({
    Name = "テレポートアップ",
    Default = false,
    Callback = function(Value)
        MainTab.ToggleState = Value
        print("Toggleの状態が変更されました:", Value)
    end    
})

