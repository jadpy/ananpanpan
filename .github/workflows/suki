ProTab:AddButton({
        Name = "高速上下回転",
        Callback = function()

local gui = Instance.new("ScreenGui")
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = game.Players.LocalPlayer.PlayerGui

local button = Instance.new("TextButton")
button.Text = "Rotate"
button.Size = UDim2.new(0, 150, 0, 50)
button.Position = UDim2.new(0, 10, 1, -60)
button.AnchorPoint = Vector2.new(0, 1)
button.BackgroundColor3 = Color3.fromRGB(173, 216, 230) -- MistyBlue色
button.Parent = gui

-- ボタンがクリックされたときの処理
button.MouseButton1Click:Connect(function()
    -- 乗っている乗り物を取得する
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        local seatPart = humanoid and humanoid.SeatPart
        if seatPart then
            local vehicle = seatPart.Parent
            if vehicle then
                -- 乗り物を永遠に順番に高速に回転させる
                while true do
                    for i = 1, 3 do
                        vehicle:SetPrimaryPartCFrame(vehicle.PrimaryPart.CFrame * CFrame.Angles(math.rad(60), 0, 0))
                        wait(0.0001) -- 0.0001秒待つ（超高速化）
                    end
                end
            end
        end
    end
end)
          end    
})


local ProTab = Window:MakeTab({
        Name = "アンチスクリプト",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

ProTab:AddButton({
        Name = "アンチキックフライ",
        Callback = function()

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local camera = game.Workspace.CurrentCamera

while wait(0.1) do
    local currentPosition = humanoidRootPart.Position
    local forwardDirection = camera.CFrame.LookVector
    local rightDirection = camera.CFrame.RightVector
    local upDirection = Vector3.new(0, 1, 0)
    local downDirection = Vector3.new(0, -1, 0)
    local newPosition = currentPosition

    -- 歩いてる方向に3mTP
    newPosition = newPosition + forwardDirection * 3

    -- 向いている方向に5mずつTP
    if forwardDirection:Dot(upDirection) > 0.5 then
        newPosition = newPosition + upDirection * 5
    end

    -- 上を向いた時に上にTP
    if forwardDirection:Dot(upDirection) > 0.5 then
        newPosition = newPosition + upDirection * 5
    end

    -- 下を向いた時に下にTP
    if forwardDirection:Dot(downDirection) > 0.5 then
        newPosition = newPosition + downDirection * 5
    end

    -- 後ろを向いた時に下にTP
    if forwardDirection:Dot(-upDirection) > 0.5 then
        newPosition = newPosition + downDirection * 5
    end

    character:SetPrimaryPartCFrame(CFrame.new(newPosition))
end

          end    
})
ProTab:AddButton({
        Name = "アンチチートテレポートBypass",
        Callback = function()

local cords = Vector3.new(x,y,z)
--script
local plr = game:service("Players").LocalPlayer
function Time(targetpos)
    local tme = (targetpos - plr.Character.HumanoidRootPart.Position).Magnitude / plr.Character:FindFirstChildOfClass("Humanoid").WalkSpeed
    return tme
end

local tween_s = game:service("TweenService")
local info = TweenInfo.new(Time(cords),Enum.EasingStyle.Quad)
function tp(...)
local tic_k = tick();
local params = {...};
local cframe = CFrame.new(params[1],params[2],params[3]);
local tween,err = pcall(function()
local tween = tween_s:Create(plr.Character["HumanoidRootPart"],info,{CFrame=cframe});
      tween:Play();
end)
   if not tween then return err end
end

tp(cords)


          end    
})

