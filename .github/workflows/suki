            local character = part_to_impulse.Parent
            local rootPart = character:FindFirstChild("HumanoidRootPart")
            if rootPart then
                -- テレポート先の上の位置を計算
                local upVector = Vector3.new(0, -100, 0) -- 上に20単位移動するベクトル
                local newPosition = rootPart.Position + upVector

                -- プレイヤーをテレポート
                rootPart.CFrame = CFrame.new(newPosition)

